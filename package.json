{
  "name": "baray",
  "version": "1.2.1",
  "description": "Baray - a minimilistic logger",
  "main": "index.js",
  "scripts": {
    "report": "node_modules/.bin/nyc npm test && node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && node_modules/.bin/codecov",
    "test": "node_modules/.bin/mocha tests/**/*.js --exit",
    "coverage": "node_modules/.bin/nyc npm test && node_modules/.bin/nyc --clean -s check-coverage"
  },
  "author": "Aneesh MG",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "codecov": "^2.3.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "winston": "^2.4.0"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneeshmg/Baray.git"
  },
  "keywords": [
    "logger",
    "minimalistic",
    "simple"
  ],
  "bugs": {
    "url": "https://github.com/aneeshmg/Baray/issues"
  },
  "homepage": "https://github.com/aneeshmg/Baray#readme"
}
